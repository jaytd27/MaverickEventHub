    android:layout_margin="1dp"
    android:layout_weight="1"
    android:background="#494949" >

    
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical" >
			    
			    <LinearLayout
